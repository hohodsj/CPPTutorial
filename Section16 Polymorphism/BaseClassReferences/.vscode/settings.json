{
    "C_Cpp.errorSquiggles": "disabled",
    "files.associations": {
        "*.ejs": "html",
        "*.html": "html",
        "vector": "cpp",
        "string": "cpp",
        "iostream": "cpp"
    },
    "C_Cpp.default.cppStandard": "c++17",
    "code-runner.executorMap": {
        "cpp": "cd $dir && /usr/bin/clang++ -std=c++17 -Wall -g *.cpp -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    }
}